<table>
  <th>Name</th>
  <th>Year Completed</th>
  <tr>
    <% @landmark.each do |land| %>
      <td align="center"><a href="/landmarks/<%= land.id %>"><%= land.name %></a></td>
      
      <td align="center"><%= land.year_completed %></td>
  </tr>
  <% end %>
</table>
